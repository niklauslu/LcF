{
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "dev"
  },
  "execMap": {
    "": "node",
    "js": "node"
  },
  "watch": [
    "app/*"
  ],
  "ignore": [
    ".svn",
    ".git",
    ".idea",
    "node_modules/",
    "app/views",
    "src/*",
    "js/*",
    "css/*"
  ],
  "ext": "js jade json"
}